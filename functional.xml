<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="PentagonProfileStats - Functional" parallel="false">
    <listeners>
        <listener class-name="org.pentagonprofilestats.utilities.ExtentManager"/>
        <listener class-name="io.qameta.allure.testng.AllureTestNg"/>
    </listeners>
    <test name="Functional Tests">
        <parameter name="os" value="windows"/>
        <parameter name="browser" value="chrome"/>
        <groups>
            <run>
                <include name="functional"/>
            </run>
        </groups>
        <packages>
            <package name="org.pentagonprofilestats.testCases"/>
        </packages>
    </test>
</suite>
